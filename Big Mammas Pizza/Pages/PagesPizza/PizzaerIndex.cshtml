@page
@using Big_Mammas_Pizza.model;
@model Big_Mammas_Pizza.Pages.PagesPizza.PizzaerIndexModel
@{
}
<table class="table table-hover">
    <thead>
        <tr>
            <th>Pizzanumber</th>
            <th>Name</th>
            <th>Description</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Pizza p in Model.Pizzaer)
        {
            <tr>
                <td>@p.PizzaNumber</td>
                <td>@p.Name</td>
                <td>@p.Description</td>
                <td>@p.Price</td>
            </tr>
        }


    </tbody>



</table>
